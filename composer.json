{
    "name": "se/opentrans-bundle",
    "type": "bundle",
    "description": "This bundle integrates the opentrans library into Symfony2. Allows to create OpenTRANS compatible documents from PHP.",
    "keywords": ["symfony2", "opentrans", "serialization", "xml", "bundle"],
    "homepage": "https://github.com/sveneisenschmidt/opentrans-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Eisenschmidt",
            "email": "sven.eisenschmidt@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "se/opentrans": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*|~5.7.5",
        "symfony/symfony": "2.3.*|^3.0"
    },
    "autoload": {
        "psr-0": {
            "SE\\Bundle\\OpenTransBundle": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/hefekranz/opentrans.git"
        }
    ]
}
